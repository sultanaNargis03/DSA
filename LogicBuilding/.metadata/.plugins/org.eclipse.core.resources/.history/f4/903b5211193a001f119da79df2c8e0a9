import java.util.Scanner;

public class TrailingZeroFactorial 
{
	public static int Factorial(int n)
	{
		int fact=1;
		for(int i=1;i<=n;i++)
		{
			fact=fact*i;
		}
		return fact;
				
	}
	public static int TrailingZero(int n)
	{
		int res=0;
		int power=5;
		while(power<=n)
		{
			res=res+(power/5);
			power=power*5;
		}
		return res;
	}
	public static void main(String[] args) 
	{
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number to count the trailing zero from its factorial");
		int n=sc.nextInt();
		System.out.println("Factorial of "+n+" is: "+Factorial(n));
		System.out.println("Trailing zero of factorial of "+n+" is: "+Factorial(n));
	}

}
