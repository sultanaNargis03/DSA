
public class FindElement 
{
	int findSecondLargestElement(int ar[]) {
		
		int max1=ar[0];
		int max2=ar[1];
		
		for(int i=2;i<ar.length;i++)
		{
			if(max1>max2)
			{
				if(ar[i]>max1)
				{
					max1=ar[i];
					max2=max1;
				}
				else if(ar[i]>max2)
				{
					max2=ar[i];
				}
			}
			else
			{
				int temp=max1;
				max1=max2;
				max2=temp;
			}
		}
	
		
		return max2;
	}
}
